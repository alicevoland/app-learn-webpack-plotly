<!DOCTYPE html>
<html>
  <head>
    <title>My App</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_pack_tag 'application', media: 'all' %>
  </head>

  <body>

    <%= render partial: 'shared/navbar'%>
    <%= yield :jumbotron %>
    <%= render partial: 'shared/alerts'%>

    <%= content_for?(:application) ? (yield :application) : (yield) %>

    <%= javascript_pack_tag 'application' %>
  </body>
</html>
